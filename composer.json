{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "berkayk/onesignal-laravel": "^1.0",
        "ccxt/ccxt": "^1.83",
        "coingate/coingate-php": "^3.0",
        "cviebrock/eloquent-sluggable": "^9.0",
        "dedoc/scramble": "^0.7.0",
        "doctrine/dbal": "^3.5",
        "ezyang/htmlpurifier": "^4.13",
        "fideloper/proxy": "^4.4",
        "flowgistics/laravel-xml": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "3.0.0-alpha.5",
        "kornrunner/keccak": "^1.1",
        "laracasts/utilities": "^3.2",
        "laravel/fortify": "^1.14",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.13",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mailjet/mailjet-apiv3-php": "^1.5",
        "messagebird/php-rest-api": "^3.1",
        "mollie/laravel-mollie": "^2.17",
        "paypal/paypal-checkout-sdk": "^1.0",
        "phpmailer/phpmailer": "^6.6",
        "pusher/pusher-php-server": "^7.2",
        "rappasoft/laravel-livewire-tables": "^2.8",
        "razorpay/razorpay": "^2.5",
        "sendgrid/sendgrid": "^8.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "simplito/elliptic-php": "^1.0",
        "stripe/stripe-php": "^7.72",
        "tatumio/tatum-php": "^1.0",
        "textmagic/sdk": "dev-master",
        "twilio/sdk": "^6.43",
        "vonage/client": "^3.0",
        "yajra/laravel-datatables-oracle": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/envoy": "^2.8",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "repositories": {
        "tatumio/tatum-php": {
            "type": "path",
            "url": "app/Http/Controllers/Admin/Eco/tatum"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
