{
    "name": "tatumio/tatum-php",
    "description": "PHP SDK to Tatum.io API client, with examples",
    "type": "library",
    "homepage": "https://tatum.io",
    "version": "1.0.5",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-gmp": "^8.0",
        "ext-openssl": "*",
        "ext-mbstring": "^8.0",
        "mbezhanov/ethereum-converter": "dev-master",
        "web3p/ethereum-tx": "dev-master"
    },
    "support": {
        "issues": "https://github.com/tatumio/tatum-php/issues",
        "source": "https://github.com/tatumio/tatum-php"
    },
    "autoload": {
        "psr-4": {
            "Tatum\\": "src"
        },
        "classmap": [
            "src/Tatum.php"
        ]
    },
    "license": "MIT",
    "authors": [{
            "name": "Adeleye Benjamin Adesanoye",
            "email": "adeleye.benjamin@highbreedtech.com",
            "role": "Author"
        },
        {
            "name": "High Breed Technology Limited",
            "email": "support@highbreedtech.com",
            "role": "Author's Team"
        },
        {
            "name": "Tatum",
            "email": "hello@tatum.io",
            "role": "Team"
        }
    ],
    "keywords": [
        "Tatum",
        "Tatumio",
        "Tatum.io",
        "Blockchain",
        "Building Blocks",
        "Bitcoin",
        "Litecoin",
        "Stellar",
        "Tatum API",
        "XRP",
        "Ethereum",
        "Tron"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}